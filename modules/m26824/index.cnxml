               <m:mi>ω</m:mi>
               <m:mn>2</m:mn>
             </m:msub>
           </m:mrow>
           <m:mo fence="true" form="postfix" stretchy="false">)</m:mo>
         </m:mrow>
       </m:mrow>
     </m:mrow>
   </m:math>
   
