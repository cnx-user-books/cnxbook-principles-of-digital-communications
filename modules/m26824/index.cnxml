         <m:mo/>
         <m:mi>π</m:mi>
         <m:mo/>
         <m:mi>δ</m:mi>
         <m:mo/>
         <m:mrow>
           <m:mo fence="true" form="prefix" stretchy="false">(</m:mo>
           <m:mrow>
             <m:msub>
               <m:mi>ω</m:mi>
               <m:mn>1</m:mn>
             </m:msub>
             <m:mo form="infix">−</m:mo>
